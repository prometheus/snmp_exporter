        }
AdGenGponOntPortAutoDetectMode ::= TEXTUAL-CONVENTION
        STATUS  deprecated
        DESCRIPTION
            "Indicates auto detection configuration of ONT Ethernet interface."
        SYNTAX   INTEGER {
                 autoAuto (0),
                 tenFull (1),
                 hundredFull (2),
                 thousandFull (3),
                 autoFull (4),
                 tenGigabitFull (5),
                 twentyFiveHundredFull (6),
                 fiveGigabitFull (7),
                 tenHalf (17),
